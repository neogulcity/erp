<!doctype html>
<html lang="ko">

<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <link rel = "stylesheet" href = "css/iframe.css">
</head>

<body>
    <table class ="">
        <thead>
            <tr>
                <th class="cols">거래처 ID</th>
                <th class="cols">거래처 명</th>
            </tr>
        </thead>
        <tbody id = "loop-corporate-list">
            <% data.forEach(function (elem) { %>
                <tr>
                    <td class = "cell"><%= elem["거래처 ID"] %></td>
                    <td class = "cell"><%= elem["거래처 명"] %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
    <script>
        window.addEventListener('message', ReceiveMsg);

        function ReceiveMsg(msg) {
            let form = document.createElement('form');
            form.action = '/Search';
            form.method = 'POST';
            form.acceptCharset = "UTF-8";

            let textData = '<input type="hidden" name="page" value="ViewCorppage">';
            textData += '<input type="hidden" name="table" value="거래처">';
            textData += '<input type="hidden" name="target" value=' + msg.data + '>';
            form.innerHTML = textData;
            document.body.append(form);
            form.submit();
        }
    </script>
</body>
</html>